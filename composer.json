{
  "name": "railroad/railforums",
  "description": "Laravel forum system using entities",
  "license": "MIT",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/railroadmedia/response"
    }
  ],
  "authors": [
    {
      "name": "Caleb Favor",
      "email": "calebfavor@gmail.com"
    }
  ],
  "require": {
    "php": "~7.1.3",
    "laravel/framework": "5.6.*",
    "guzzlehttp/guzzle": "~6.0",
    "doctrine/dbal": "^2.5",
    "ezyang/htmlpurifier": "^4.10",
    "railroad/railmap": "dev-master",
    "railroad/railnotifications": "dev-master",
    "railroad/railsettings": "dev-master",
    "railroad/resora": "0.0.05",
    "railroad/permissions": "0.0.02",
    "spatie/laravel-fractal": "^5.3",
    "railroad/response": "0.0.07"
  },
  "require-dev": {
    "phpunit/phpunit": "^7.0",
    "orchestra/testbench": "3.6.*@dev",
    "orchestra/database": "3.6.*@dev"
  },
  "autoload": {
    "psr-4": {
      "Railroad\\Railforums\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  }
}
